{% extends 'base.html.twig' %}

{% block title %} {% endblock %}

{% block body %}

<div class="example-wrapper">
    <h1>{{ pluralize(pins|length ,'Pin') }} </h1>

    {% for pin in pins %}
    <article>
        <h2><a href="{{path('app_pins_show',{id:pin.id})}}" title="{{pin.title}}">{{pin.title}}</a></h2>
        <p>{{pin.description}}</p>
    </article>
    {% endfor %}
</div>
{% endblock %}